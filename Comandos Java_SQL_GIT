        "Crear repositorio\n",
        "\n",
        "\tgit init\n",
        "\n",
        "Estatus del repositorio\n",
        "\n",
        "\tgit status\n",
        "\n",
        "Rastrear/añadir archivo especifico\n",
        "\n",
        "\tgit add <file>\n",
        "\n",
        "Rastrear/añadir archivo total\n",
        "\n",
        "\tgit add .\n",
        "\n",
        "Dejar de rastrear archivo\n",
        "\n",
        "\tgit rm --cached <file>\n",
        "\n",
        "Editar local nombre / email\n",
        "\n",
        "\tgit config --local user.name \"<file>\"\n",
        "\tgit config --local user.email \"<file>\"\n",
        "\n",
        "Chekpoint\n",
        "\n",
